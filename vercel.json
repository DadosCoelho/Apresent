{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:css|js|png|jpg|jpeg|gif|svg|ico|html))$",
      "dest": "/public/$1"
    },
    {
      "src": "/apresentacoes/(.*)",
      "dest": "/public/apresentacoes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}